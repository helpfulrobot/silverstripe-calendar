<div id="$NavigationBarID" class="calendarNavigationBar">
	<div class="previous ajaxified">
		<a rel="$PrevLink" title="View the previous $ViewTitle">&lt; Previous</a>
	</div>
	<div class="current">
		<span>$ViewDateTitle</span>
	</div>
	<div class="next ajaxified">
		<a rel="$NextLink" title="View the next $ViewTitle">Next &gt;</a>
	</div>
</div>
